{
	"compilerOptions": {
		"target": "es5",
		"lib": ["es6"],
		"module": "commonjs",
		"types": ["reflect-metadata"],
		"rootDir": "./src",
		"outDir": "./build",
		"moduleResolution": "node",
        "experimentalDecorators": true,
        "emitDecoratorMetadata": true,
		"esModuleInterop": true,
		"resolveJsonModule": true,
		"strictPropertyInitialization": false,
		"noImplicitAny": false,
		"strict": true,
		"sourceMap": true
	}
}